{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        // использование более строгого вывода типов для свойств данных в `this`
        "strict": true,
        "jsx": "preserve",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "noImplicitAny": false,
    },
    "include": ["**/*.ts", "**/*.tsx", "**/*.vue", "images.d.ts", "svg.d.ts"],
  }